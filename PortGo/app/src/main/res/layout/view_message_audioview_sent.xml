<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    >
<ImageView xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/message_audio_send_content"
    android:src="@drawable/voice_palyr"
    android:layout_height="wrap_content"
    android:layout_width="wrap_content"
    android:minHeight="@dimen/chat_item_height"
    android:minWidth="@dimen/chat_content_minwidth"
    android:gravity="left|center_vertical"
    android:paddingLeft="@dimen/chat_text_padding_h"
    android:paddingRight="@dimen/chat_text_padding_h"
    android:paddingTop="@dimen/chat_text_padding_v"
    android:paddingBottom="@dimen/chat_text_padding_v"
    android:scaleType="fitCenter"
    android:textSize="@dimen/chat_content_textsize"
    android:layout_marginLeft="@dimen/message_text_margin_failed"/>
</FrameLayout>
