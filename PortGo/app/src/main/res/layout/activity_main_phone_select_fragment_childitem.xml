<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:orientation="horizontal"
	android:id="@+id/activity_main_contact_fragment"
	android:background="@drawable/list_selector">
	<!--include android:id="@+id/titleLayout" layout="@layout/view_tab_header" /-->
	<CheckBox
		android:id="@+id/phone_item_check"
		android:button="@drawable/checkbox_bg"
		android:layout_marginLeft="100dp"
		android:layout_width="wrap_content"
		android:layout_height="wrap_content"
		android:layout_marginRight="@dimen/contact_item_radiobox_marginright"
		android:focusable="false"
		android:clickable="false"/>
	<TextView
		android:id="@+id/child_item_title"
		android:layout_width="match_parent"
		android:gravity="center_vertical"

		android:layout_height="@dimen/min_item_height"/>

</LinearLayout>
