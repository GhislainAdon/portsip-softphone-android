<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="oval">
            <solid android:color="@color/portgo_color_blue" />
            <size android:scaleType="fitXY" android:height="@dimen/detail_round_btn_radius" android:width="@dimen/detail_round_btn_radius" />
        </shape>
    </item>
    <item>
        <shape android:shape="oval">
            <corners android:radius="@dimen/detail_round_btn_radius" />
            <solid android:color="@color/portgo_color_blue" />
            <size android:scaleType="fitXY" android:height="@dimen/detail_round_btn_radius" android:width="@dimen/detail_round_btn_radius" />
        </shape>
    </item>
</selector>